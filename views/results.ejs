<%-include("partials/header")%>
<!-- <div class="bg-image"></div> -->
<div class="container">
		<div class="jumbotron">
			<h1>Showing results for "<%=song%>"</h1>
		</div>
		<div class="row text-center" style="display:flex; flex-wrap:wrap">
			<%val.tracks.items.forEach(function(dt){%>
				<div class="col-md-3 col-sm-6">
					<a href="/song/<%=dt.id%>">
					<div class="thumbnail">
						<img src=<%=dt.album.images[1].url%>>
						<div class="caption">
							<p><strong>Album Name : </strong><%=dt.album.name.substring(0,30)%></p>
							<strong>Artists : </strong>
							<%dt.artists.forEach(function(art){%>
								<%=art.name%>,
							<%});%>
							
						</div>
					</div>
					</a>
				</div>
			<%});%>
		</div>
</div>

<%-include("partials/footer")%>
